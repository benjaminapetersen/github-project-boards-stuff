# GitHub Personal Access Token
# Required: repo, read:org, project scopes
export GITHUB_TOKEN=ghp_your_token_here

# Comma-separated list of GitHub usernames to search for activity
export GITHUB_USERNAMES=your-username

# GitHub Project ID (optional)
# Only needed if you want to add items to a project board
# export PROJECT_ID=PVT_your_project_id
